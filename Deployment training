   // Client implementation
   import createGrokL from '@grok-l/sdk';
   
   const grokl = createGrokL({
       apiKey: 'your-api-key',
       coherenceThreshold: 0.88
   });
   
   // Capture audio sovereignty signature
   const audioFile = document.getElementById('audio-input').files[0];
   const signature = await grokl.captureSignature(
       audioFile, 
       'audio',
       1.0  // Peak consciousness state
